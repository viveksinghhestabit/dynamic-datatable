{
    "name": "viveksingh/dynamic-datatable",
    "description": "dynamic datatable",
    "keywords": [
        "viveksingh",
        "dynamic-datatable"
    ],
    "homepage": "https://github.com/viveksingh/dynamic-datatable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vivek Singh",
            "email": "viveksingh.hestabit@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "laravel/database": "^8.83"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Viveksingh\\DynamicDatatable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Viveksingh\\DynamicDatatable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Viveksingh\\DynamicDatatable\\DynamicDatatableServiceProvider"
            ],
            "aliases": {
                "DynamicDatatable": "Viveksingh\\DynamicDatatable\\DynamicDatatableFacade"
            }
        }
    }
}
